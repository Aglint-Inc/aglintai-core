{
  "name": "aglint-core",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "autofix": "eslint . --fix"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^1.12.4"
  },
  "packageManager": "npm@8.19.4",
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  }
}
