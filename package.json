{
  "name": "aglint-core",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "supabase:dev": "npx supabase gen types typescript --project-id plionpfmgvenmdwwjzac --schema public > packages/shared-types/src/db/schema.types.ts",
    "supabase:local": "npx supabase gen types typescript --local > packages/shared-types/src/db/schema.types.ts",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "autofix": "eslint . --fix",
    "pull": "git pull origin development --no-rebase"
  },
  "devDependencies": {
    "prettier": "^3.3.2",
    "turbo": "^2.0.4"
  },
  "packageManager": "npm@10.5.2",
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "dependencies": {}
}
