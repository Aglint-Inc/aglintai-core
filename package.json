{
  "name": "aglint-core",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "build:api": "turbo run build --filter=aglint-apis",
    "start:api": "turbo run start:development --filter=aglint-apis",
    "supabase": "npx supabase gen types typescript --project-id plionpfmgvenmdwwjzac --schema public > packages/shared-types/src/schema.ts",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "autofix": "eslint . --fix",
    "pull": "git pull origin development --no-rebase"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^1.12.4"
  },
  "packageManager": "npm@8.19.4",
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "dependencies": {
    "dayjs": "^1.11.11"
  }
}
