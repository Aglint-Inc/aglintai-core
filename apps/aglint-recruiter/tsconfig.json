{
  "extends": "@repo/typescript-config/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@devlink": ["./devlink/*"],
      "@components/*": ["./src/components/*"],
      "@config/*": ["./src/config/*"],
      "@context/*": ["./src/context/*"],
      "@defaultData/*": ["./src/defaultData/*"],
      "@styles/*": ["./styles/*"],
      "@utils/*": ["./src/utils/*"],
      "@public/*": ["./public/*"]
    },
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "next.config.js",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",
    "**/*.js",
    "src/components/Feedback/Session/index.jsx",
    "src/pages/api/getJobsPublic.js"
  ],
  "exclude": ["node_modules"]
}
