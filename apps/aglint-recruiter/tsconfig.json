{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noErrorTruncation": true,
    "jsx": "preserve",
    "paths": {
      "@/*": ["./*"],
      "@devlink": ["./devlink/*"],
      "@components/*": ["./src/components/*"],
      "@config/*": ["./src/config/*"],
      "@context/*": ["./src/context/*"],
      "@defaultData/*": ["./src/defaultData/*"],
      "@styles/*": ["./styles/*"],
      "@utils/*": ["./src/utils/*"],
      "@public/*": ["./public/*"],
      "@types/*": ["./src/types/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": false
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",
    "**/*.js",
    "src/components/Feedback/Session/index.jsx",
    "src/pages/api/getJobsPublic.js",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
