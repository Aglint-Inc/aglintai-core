{
  "name": "aglint-recruiter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run gen_path && npm run dev:build && next dev",
    "build": "next build",
    "start": "next start",
    "dev:build": "npm run build --workspace=@aglint/shared-utils",
    "type-check": "npx tsc --noEmit",
    "tslint": "next lint && npx tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "eslint . --cache  --ext .ts,.js,.tsx,.jsx --fix",
    "lint:no-circular": "eslint . --rule \"import/no-cycle: off\" && npm run tslint",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "supabase:dev": "npx supabase gen types --lang=typescript --project-id ecfwsyxpcuzxlxrkhxjz --schema public > ../../packages/shared-types/src/db/schema.types.ts && npm run fix_types && npm run generate-zod-schemas",
    "supabase:local": "npx supabase gen types --lang=typescript --local  --schema public> ../../packages/shared-types/src/db/schema.types.ts && npm run fix_types && npm run generate-zod-schemas",
    "fix_types": "npx tsc script/removeJsonType.ts --skipLibCheck && node script/removeJsonType.js",
    "pull": "git pull origin development --no-rebase",
    "gen_path": "npx tsc script/path_gen.ts --skipLibCheck && node script/path_gen.js && npm run script_cleanup",
    "gen_types": "npx tsc script/genTypesFromDB.ts --skipLibCheck && node script/genTypesFromDB.js",
    "shadcn": "npx shadcn-ui@latest add",
    "v0": "npx v0@latest add",
    "script_cleanup": "npx tsc script/cleanup.ts --skipLibCheck && node script/cleanup.js",
    "generate-zod-schemas": "supabase-to-zod --input ../../packages/shared-types/src/db/schema.types.ts --output ../../packages/shared-types/src/db/zod-schema.types.ts"
  },
