        "dlx": "^0.2.1",
        "dotenv": "^16.3.1",
        "langchain": "^0.0.174",
        "openai": "^4.14.0"
    },
    "devDependencies": {
        "nodemon": "^2.0.15",
        "typescript": "^5.2.2"
    }
