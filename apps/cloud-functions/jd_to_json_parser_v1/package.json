{
  "name": "jd_parser",
  "version": "1.0.0",
  "description": "parser function for jd to jd json",
  "packageManager": "yarn@1.22.19",
  "engines": {
    "yarn": "^1.22.19"
  },
  "main": "lib/index.js",
  "author": "Vivek√ü",
  "license": "",
  "private": true,
  "scripts": {
    "tsconfig": "tsc --init",
    "gcp-build": "tsc",
    "start": "tsc && functions-framework --target hello --port 8084",
    "dev": "nodemon --ext ts --exec 'yarn start'",
    "deploye": "gcloud functions deploy jd-to-json-parser --gen2 --runtime=nodejs18 --region=northamerica-northeast2 --source=. --entry-point=hello --trigger-http --allow-unauthenticated"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0",
    "@supabase/supabase-js": "^2.38.5",
    "dlx": "^0.2.1",
    "dotenv": "^16.3.1",
    "langchain": "^0.0.194",
    "openai": "^4.19.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "typescript": "^5.2.2"
  }
}
