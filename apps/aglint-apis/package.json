{
  "name": "aglint-apis",
  "version": "1.0.0",
  "scripts": {
    "dev:build": "npm run build --workspace=@aglint/shared-utils",
    "dev": "npm run dev:build && tsup --watch --onSuccess \"node dist/index.js\" && console-ninja",
    "build": "tsup",
    "lint": "gts lint",
    "type-check": "tsup",
    "start": "set NODE_ENV=development&& node dist/index.js",
    "start:production": "set NODE_ENV=production&& node dist/index.js",
    "start:staging": "set NODE_ENV=staging&& node dist/index.js",
    "start:development": "set NODE_ENV=development&& node  dist/index.js",
    "compile": "tsc",
    "fix": "gts fix"
  },
  "dependencies": {
    "@aglint/shared-utils": "*",
    "@langchain/community": "^0.3.10",
    "@langchain/core": "^0.1.63",
    "@langchain/langgraph": "^0.0.31",
    "@langchain/openai": "^0.0.29",
    "@supabase/supabase-js": "^2.39.8",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "axios": "^1.7.4",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.1",
    "express": "^4.19.2",
    "express-ws": "^5.0.2",
    "gts": "^5.3.0",
    "ioredis": "^5.4.1",
    "langchain": "^0.1.28",
    "openai": "^4.67.1",
    "prettier": "^3.2.5",
    "retell-client-js-sdk": "^1.3.2",
    "retell-sdk": "2.4.0",
    "supabase": "^1.150.0",
    "twilio": "^4.21.0",
    "uuidv4": "^6.2.13",
    "winston": "^3.13.0",
    "ws": "^8.18.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.5",
    "zod-validation-error": "^3.2.0"
  },
  "devDependencies": {
    "@aglint/shared-types": "*",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.4",
    "@types/node": "20.11.5",
    "@typescript-eslint/eslint-plugin": "^5.7.1-alpha.25",
    "eslint": "^8.57.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.4",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "packageManager": "npm@8.19.4"
}
